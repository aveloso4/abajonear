[  {    "type": "post",    "url": "/auth",    "title": "Get a new token",    "version": "0.1.0",    "name": "GetToken",    "group": "Auth",    "permission": [      {        "name": "none"      }    ],    "description": "<p>In order to use the api you will need to authenticate to submit a JSWT on each request.</p> ",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "email",            "description": "<p>Email of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "password",            "description": "<p>Password of the User.</p> "          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "<p>Bool</p> ",            "optional": false,            "field": "success",            "description": "<p>The auth was succesfully.</p> "          },          {            "group": "Success 200",            "type": "<p>String[]</p> ",            "optional": false,            "field": "tokens",            "description": "<p>Returns two tokens, access_token and refresh_token.</p> "          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n  \"success\": true,\n  \"tokens\": {\n     \"access_token\": \"eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJfaWQiOiI1NjU5NTZiYjI1MjBiNGJkMDI1ZGMwMmQiLCJlbWFpbCI6Im5kdXF1ZUBkb2RhYmxlLmNvbSIsInBhc3N3b3JkIjoiJDJhJDEwJFlXMGtRTW82bFdxNC9wZVhIY3BEMU8zWEJRbTdXbDM1NXdrNGN4TlBjMXdmWmlYRDVpRloyIiwiZGlzcGxheV9uYW1lIjoidW5kZWZpbmVkIHVuZGVmaW5lZCIsIl9fdiI6MCwiYWRkcmVzc2VzIjpbXSwidXNlcl90eXBlIjoidXNlciJ9.sfyDmCWoh_x58jSt3YLlMw8mDLvXV2CMk8rjUcb-O-E\",\n     \"refresh_token\": \"eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJfaWQiOiI1NjU5NTZiYjI1MjBiNGJkMDI1ZGMwMmQiLCJlbWFpbCI6Im5kdXF1ZUBkb2RhYmxlLmNvbSIsInBhc3N3b3JkIjoiJDJhJDEwJFlXMGtRTW82bFdxNC9wZVhIY3BEMU8zWEJRbTdXbDM1NXdrNGN4TlBjMXdmWmlYRDVpRloyIiwiZGlzcGxheV9uYW1lIjoidW5kZWZpbmVkIHVuZGVmaW5lZCIsIl9fdiI6MCwiYWRkcmVzc2VzIjpbXSwidXNlcl90eXBlIjoidXNlciJ9.DyXtwx0gY5Lxxix5Pp5jUSj9qhs40FhOQ4QffluKRYU\"\n  }    \n}",          "type": "json"        }      ]    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "UserNotFound",            "description": "<p>The email of the User was not found.</p> "          },          {            "group": "Error 4xx",            "optional": false,            "field": "AuthenticationFailed",            "description": "<p>The password did not match</p> "          }        ]      },      "examples": [        {          "title": "Error-Response: UserNotFound",          "content": "HTTP/1.1 404 Not Found\n{\n  \"error\": \"UserNotFound\"\n}",          "type": "json"        },        {          "title": "Error-Response: AuthenticationFailed",          "content": "HTTP/1.1 401 Unauthorized\n{\n  \"error\": \"AuthenticationFailed\"\n}",          "type": "json"        }      ]    },    "filename": "app/routes/auth.js",    "groupTitle": "Auth"  },  {    "type": "get",    "url": "/users/:userId",    "title": "Read data of a User",    "version": "0.1.0",    "name": "GetUser",    "group": "User",    "permission": [      {        "name": "admin"      }    ],    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "id",            "description": "<p>The Users-ID.</p> "          }        ]      }    },    "examples": [      {        "title": "Example usage:",        "content": "curl -i http://abajonear-api.herokuapp.com/users/AABBCCDFDF",        "type": "json"      }    ],    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "_id",            "description": "<p>The Users-ID.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "email",            "description": "<p>Email of the user.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "first_name",            "description": "<p>First name of the User.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "last_name",            "description": "<p>Last name of the User.</p> "          },          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "user_type",            "description": "<p>Role of the User.</p> "          },          {            "group": "Success 200",            "type": "<p>Object[]</p> ",            "optional": false,            "field": "addressess",            "description": "<p>List of Users address (Array of Objects).</p> "          },          {            "group": "Success 200",            "type": "<p>ObjectId</p> ",            "optional": false,            "field": "_company",            "description": "<p>The company Id which user belongs to.</p> "          }        ]      }    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "NoAccessRight",            "description": "<p>Only authenticated Admins can access the data.</p> "          },          {            "group": "Error 4xx",            "optional": false,            "field": "UserNotFound",            "description": "<p>The <code>id</code> of the User was not found.</p> "          }        ]      },      "examples": [        {          "title": "Response (example):",          "content": "HTTP/1.1 401 Not Authenticated\n{\n  \"error\": \"NoAccessRight\"\n}",          "type": "json"        }      ]    },    "filename": "app/routes/users.js",    "groupTitle": "User"  },  {    "type": "post",    "url": "/users",    "title": "Create a new User",    "version": "0.1.0",    "name": "PostUser",    "group": "User",    "permission": [      {        "name": "admin"      }    ],    "description": "<p>In this case &quot;apiUse&quot; is defined and used. Define blocks with params that will be used in several functions, so you dont have to rewrite them.</p> ",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "email",            "description": "<p>Email of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "password",            "description": "<p>Password of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "first_name",            "description": "<p>First Name of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "last_name",            "description": "<p>Last Name of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "user_type",            "description": "<p>Role of the User.</p> "          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "id",            "description": "<p>The new Users-ID.</p> "          }        ]      }    },    "filename": "app/routes/users.js",    "groupTitle": "User"  },  {    "type": "put",    "url": "/users/:userId",    "title": "Update an User",    "version": "0.1.0",    "name": "PutUser",    "group": "User",    "permission": [      {        "name": "none"      }    ],    "description": "<p>This function has same errors like POST /users.</p> ",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "email",            "description": "<p>Email of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "password",            "description": "<p>Password of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "first_name",            "description": "<p>First Name of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "last_name",            "description": "<p>Last Name of the User.</p> "          },          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "user_type",            "description": "<p>Role of the User.</p> "          }        ]      }    },    "filename": "app/routes/users.js",    "groupTitle": "User"  },  {    "type": "delete",    "url": "/users/:userId",    "title": "Delete an User",    "version": "0.1.0",    "name": "deleteUser",    "group": "User",    "permission": [      {        "name": "admin"      }    ],    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "<p>String</p> ",            "optional": false,            "field": "userId",            "description": "<p>User-ID.</p> "          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "<p>String</p> ",            "optional": false,            "field": "userId",            "description": "<p>The User-ID of the deleted User.</p> "          }        ]      }    },    "filename": "app/routes/users.js",    "groupTitle": "User"  }]